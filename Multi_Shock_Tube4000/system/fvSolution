FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "rho.*"
    {
        solver          diagonal;
    }

	p
	{
	    solver          GAMG;
	    tolerance       1e-10;
	    relTol          0.01;
	    smoother        GaussSeidel;
	}	
    
	p_rgh
	{
	    solver          GAMG;
	    tolerance       1e-10;
	    relTol          0.01;
	    smoother        GaussSeidel;
	}
    
    p_rghFinal
    {
        $p_rgh;
        relTol              0;
    }


    pFinal
    {
        $p;
        relTol           0.0;
    }

    "(U|h|k|epsilon|Yi.*)"
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        tolerance       1e-10;
        relTol          0.01;
    }

    "(U|h|k|epsilon|Yi.*)Final"
    {
        $U;
        relTol          0;
    }

}


PIMPLE
{
    momentumPredictor yes;
    nOuterCorrectors  4;
    nCorrectors       3;
    nNonOrthogonalCorrectors 0;
}


