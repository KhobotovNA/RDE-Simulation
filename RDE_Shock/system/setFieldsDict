FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultValues
{
    U       	 (10 0 0);
    T       	 300;
    p      	 100000;
}

zones
{
    boxToCell
    {
        type        box;

        box         (0 0 0) (0.24 0.06 0.06);

        values
        {
            T      	 1500;
            p            5000000;
            U      	 (10 0 0);
        }
    }
}

